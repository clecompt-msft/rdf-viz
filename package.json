{
  "name": "rdf-viz",
  "version": "1.1.0",
  "description": "An RDF graph visualization tool.",
  "main": "app/main.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json --fix",
    "format": "prettier --write --loglevel warn \"src/**/*.ts\" \"*.md\"",
    "prebuild": "npm run format && npm run lint",
    "build": "tsc",
    "start": "electron .",
    "pretest": "npm run build",
    "test": "npm start",
    "prepack": "npm run build",
    "pack": "electron-builder",
    "prepare": "npm run build"
  },
  "author": "Carl LeCompte",
  "license": "MIT",
  "dependencies": {
    "@rdfjs/data-model": "^1.1.1",
    "@rdfjs/formats-common": "^2.0.0",
    "cytoscape": "^3.5.0",
    "cytoscape-klay": "^3.1.2",
    "cytoscape-popper": "^1.0.4",
    "deepmerge": "^3.2.0",
    "get-stream": "^4.1.0",
    "glob": "^7.1.3",
    "minimatch": "^3.0.4",
    "nock": "^10.0.6",
    "request": "^2.88.0",
    "tippy.js": "^4.0.4",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@princjef/tslint-config": "^1.0.0",
    "@types/cytoscape": "^3.3.0",
    "@types/get-stream": "^3.0.1",
    "@types/glob": "^7.1.1",
    "@types/minimatch": "^3.0.3",
    "@types/nock": "^9.3.1",
    "@types/rdf-js": "^2.0.1",
    "@types/request": "^2.48.1",
    "@types/uuid": "^3.4.4",
    "electron": "^4.0.7",
    "electron-builder": "^20.38.5",
    "prettier": "^1.16.4",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "build": {
    "productName": "RDF Visualizer",
    "remoteBuild": false,
    "directories": {
      "buildResources": "ux",
      "output": "pkg"
    },
    "files": [
      "app",
      "ux",
      "config"
    ],
    "win": {
      "target": "portable",
      "artifactName": "${name}.${ext}"
    }
  }
}
