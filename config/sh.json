{
    "@include": "rdfs",
    "namespace": {
        "sh": "http://www.w3.org/ns/shacl#"
    },
    "style": {
        "edge": {
            "sh:targetClass": {
                "label": "",
                "line-fill": "linear-gradient",
                "line-gradient-stop-colors": "gold yellowgreen",
                "target-arrow-color": "yellowgreen"
            },
            "sh:property": {
                "label": "",
                "line-fill": "linear-gradient",
                "line-gradient-stop-colors": "gold violet",
                "target-arrow-color": "violet",
                "layout-direction": "reverse"
            },
            "sh:path": {
                "label": "",
                "line-fill": "linear-gradient",
                "line-gradient-stop-colors": "violet salmon",
                "target-arrow-color": "salmon"
            },
            "sh:node": {
                "label": "",
                "line-fill": "linear-gradient",
                "line-gradient-stop-colors": "violet gold",
                "target-arrow-color": "gold",
                "layout-direction": "reverse"
            },
            "sh:class": {
                "label": "",
                "line-fill": "linear-gradient",
                "line-gradient-stop-colors": "violet yellowgreen",
                "target-arrow-color": "yellowgreen",
                "layout-direction": "reverse"
            },
            "sh:datatype": {
                "label": "",
                "line-fill": "linear-gradient",
                "line-gradient-stop-colors": "violet silver",
                "target-arrow-color": "silver",
                "layout-direction": "reverse"
            },
            "sh:nodeKind": {
                "label": "",
                "line-fill": "linear-gradient",
                "line-gradient-stop-colors": "violet gainsboro",
                "target-arrow-color": "gainsboro",
                "layout-direction": "reverse"
            }
        },
        "node": {
            "sh:BlankNode*": {
                "node-presentation": "multiple",
                "border-color": "gainsboro"
            },
            "sh:IRI*": {
                "node-presentation": "multiple",
                "border-color": "gainsboro"
            },
            "sh:Literal": {
                "node-presentation": "multiple",
                "border-color": "gainsboro"
            }
        },
        "type": {
            "sh:NodeShape": {
                "border-color": "gold"
            },
            "sh:PropertyShape": {
                "border-color": "violet"
            }
        }
    }
}